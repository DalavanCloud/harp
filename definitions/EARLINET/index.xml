<?xml version="1.0"?>
<cd:ProductClass name="EARLINET" last-modified="2017-06-30" xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07">
  <cd:Description>EARLINET aerosol target classification products</cd:Description>

  <cd:ProductType name="EARLINET">

    <cd:ProductDefinition id="EARLINET" format="netcdf" version="3">
      <cd:Description>The format definition for this product is taken from the netcdf file itself.</cd:Description>
      <cd:DetectionRule>
        <cd:MatchExpression>regex("[a-z]{2}[0-9]{10}\\.b[0-9]+", filename())</cd:MatchExpression>
      </cd:DetectionRule>
      <cd:DetectionRule>
        <cd:MatchExpression>regex("[a-z]{2}[0-9]{10}\\.e[0-9]+", filename())</cd:MatchExpression>
      </cd:DetectionRule>
    </cd:ProductDefinition>

  </cd:ProductType>

</cd:ProductClass>
